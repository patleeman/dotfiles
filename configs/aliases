# Aliases

# ls
alias ls="ls -laGpF --color=auto"
alias ll="ls -lGpF --color=auto"
alias l="ls -lGpF --color=auto"

# Folder aliases
alias wd="cd ~/workingdir"

# Git
alias gs="git status"
alias ga="git add ."
alias gc="git commit"
alias gb="git branch"
alias gpull="git pull"

# disk usage
alias du='du -ch'