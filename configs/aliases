# Aliases

# ls
alias ls="ls -laGpF --color=auto"
alias ll="ls -lGpF --color=auto"
alias l="ls -lGpF --color=auto"

# Folder aliases
alias wd="cd ~/workingdir"

# Git
alias gs="git status"
alias ga="git add ."
alias gb="git branch"
alias gp="git pull"
alias gcm="git commit -m '$1'"

# disk usage
alias du='du -ch'